QWidget {
  background-color: #1e1e1e;
  color: #f8f8f2;
}
QPushButton {
  background-color: #3c3f41;
  border: 1px solid #5c5f61;
  padding: 4px;
  border-radius: 4px;
}
QPushButton:hover {
  background-color: #4e5255;
}
QPlainTextEdit, QListWidget, QLineEdit, QComboBox {
  background-color: #2b2b2b;
  border: 1px solid #5c5f61;
}
QMenu {
  background-color: #2b2b2b;
  color: #f8f8f2;
}
